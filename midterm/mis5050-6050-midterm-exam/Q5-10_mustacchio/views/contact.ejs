<!doctype html>
<!-- Website Template by freewebsitetemplates.com -->
<html>
<head>
	<meta charset="UTF-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<title>Contact - Mustacchio</title>
	<link rel="stylesheet" type="text/css" href="css/style.css">
	<link rel="stylesheet" type="text/css" href="css/mobile.css" media="screen and (max-width : 568px)">
	<link rel="shortcut icon" type="image/png" href="images/icons/icon-mustache.png"/>
	<script type="text/javascript" src="js/mobile.js"></script>
</head>
<body>
	
	<div id="body">
		<h1><span>let's keep in touch</span></h1>
		<form action="/contacts/create" method="post">
			<input type="text" name="name" id="fname" placeholder="name" required>
			<input type="text" name="address" id="address" placeholder="address">
			<input type="email" name="email" id="email" placeholder="email" required>
			<input type="tel" name="phone" id="phone" placeholder="phone number (XXX-XXX-XXXX)" pattern="[0-9]{3}-[0-9]{3}-[0-9]{4}">
			<textarea name="message" id="message" placeholder="MESSAGE" required></textarea>
			<input type="submit" name="send" id="send" value="send">
		</form>
	</div>
	
</body>
</html>
